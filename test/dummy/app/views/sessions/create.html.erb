<h1>Sessions#create</h1>
<p>Find me in app/views/sessions/create.html.erb</p>
<!-- app/views/sessions/new.html.erb -->
<%= form_with url: login_path do |form| %>
  <%= form.label :email %>
  <%= form.email_field :email %>
  
  <%= form.label :password %>
  <%= form.password_field :password %>
  
  <%= form.submit 'Log in' %>
<% end %>
